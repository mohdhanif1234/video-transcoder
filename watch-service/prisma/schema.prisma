generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model VideoData {
  video_id        Int      @id @default(autoincrement())
  title           String   
  description     String?
  author          String
  input_bucket_url      String
  createdAt       DateTime @default(now())
}
